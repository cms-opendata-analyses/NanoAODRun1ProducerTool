  Dimu_vtxIdx.clear();
  Dimu_chi2.clear();
  Dimu_dlxy.clear();
  Dimu_dlxyErr.clear();
  Dimu_dlxySig.clear();
  Dimu_cosphixy.clear();
  Dimu_dl.clear();
  Dimu_dlErr.clear();
  Dimu_dlSig.clear();
