  //------------------------------- For Dimu branches -----------------------//
  nDimu = 0;

if (nanoext) {

  // 1st muon
  Dimu_t1muIdx.clear();
  Dimu_t1dxy.clear();
  Dimu_t1dz.clear();

  // 2nd muon
  Dimu_t2muIdx.clear();
  Dimu_t2dxy.clear();
  Dimu_t2dz.clear();

  // Dimu
  Dimu_pt.clear();
  Dimu_eta.clear();
  Dimu_phi.clear();
  Dimu_rap.clear();
  Dimu_mass.clear();
  Dimu_charge.clear();
  Dimu_simId.clear();
